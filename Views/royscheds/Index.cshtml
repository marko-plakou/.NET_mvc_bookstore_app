@model IEnumerable<bookstore_app.Models.roysched>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.lorange)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.hirange)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.royalty)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.titles.title)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.lorange)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.hirange)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.royalty)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.titles.title)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.title_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.title_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.title_id })
        </td>
    </tr>
}

</table>
